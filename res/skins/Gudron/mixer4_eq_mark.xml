              <Pressed>mixer/eq_mark_<Variable name="i"/>.png</Pressed>
            </State>
          </PushButton>
        </Children>
      </WidgetGroup>
      <WidgetGroup>
        <Size>10min,3</Size>
        <Children>
        </Children>
      </WidgetGroup>
    </Children>
  </WidgetGroup>
