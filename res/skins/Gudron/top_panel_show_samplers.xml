<!DOCTYPE template>
<!--
  Description:
    Toggle samplers.
-->
<Template>
  <WidgetGroup>
  <Layout>horizontal</Layout>
  <Children>
    <PushButton>
      <TooltipId>show_samplers</TooltipId>
      <Style></Style>
      <NumberStates>2</NumberStates>
      <State>
        <Number>0</Number>
        <Pressed>panel/samplers_0.png</Pressed>
        <Unpressed>panel/samplers_0.png</Unpressed>
      </State>
      <State>
        <Number>1</Number>
        <Pressed>panel/samplers_1.png</Pressed>
        <Unpressed>panel/samplers_1.png</Unpressed>
      </State>
      <Connection>
        <ConfigKey>[Samplers],show_samplers</ConfigKey>
      </Connection>
    </PushButton>
    <WidgetGroup>
      <Size>8,1</Size>
    </WidgetGroup>
    <PushButton>
      <TooltipId>show_samplers</TooltipId>
      <Style></Style>
      <NumberStates>2</NumberStates>
      <State>
        <Number>0</Number>
        <Pressed>panel/samplers_2_0.png</Pressed>
        <Unpressed>panel/samplers_2_0.png</Unpressed>
      </State>
      <State>
        <Number>1</Number>
        <Pressed>panel/samplers_2_1.png</Pressed>
        <Unpressed>panel/samplers_2_1.png</Unpressed>
      </State>
      <Connection>
        <ConfigKey persist="true">[Samplers2],show_samplers</ConfigKey>
      </Connection>
    </PushButton>
    <WidgetGroup>
      <Size>7f,1</Size>
    </WidgetGroup>
  </Children>
</WidgetGroup>
</Template>
