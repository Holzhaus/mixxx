<!DOCTYPE template>
<!--
  Description:
    Toggle Deck 3 & 4.
-->
<Template>
  <WidgetGroup>
    <ObjectName>TopPanelDeck34Wide</ObjectName>
    <SizePolicy>f,f</SizePolicy>
    <Size>44,32</Size>
    <Layout>horizontal</Layout>
    <Children>
		  <PushButton>
		    <TooltipId>toggle_4decks</TooltipId>
		    <NumberStates>2</NumberStates>
		    <State>
		      <Number>0</Number>
		      <Pressed>panel/deck34_wide_1.png</Pressed>
		      <Unpressed>panel/deck34_wide_0.png</Unpressed>
		    </State>
        <State>
		      <Number>1</Number>
		      <Pressed>panel/deck34_wide_0.png</Pressed>
		      <Unpressed>panel/deck34_wide_1.png</Unpressed>
		    </State>
		    <Connection>
		      <ConfigKey>[Deck34_Wide],on</ConfigKey>
		      <EmitOnDownPress>true</EmitOnDownPress>
		      <ConnectValueToWidget>true</ConnectValueToWidget>
		    </Connection>
		  </PushButton>
    </Children>
  </WidgetGroup>
</Template>
