{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "script"
  },
  "rules": {
    "no-unused-vars": ["error", { "vars": "all", "args": "none" }],
    "quotes": ["warn", "double"],
    "eqeqeq": ["error", "always"],
    "camelcase": "error",
    "no-redeclare": "off",
    "dot-location": "off",
    "getter-return": "off",
    "require-atomic-updates": "off",
    "no-prototype-builtins": "off",
    "no-async-promise-executor": "off"
  },
  "globals": {
    "_": "readonly",
    "color": "readonly",
    "components": "readonly",
    "engine": "readonly",
    "midi": "readonly",
    "print": "readonly",
    "script": "readonly"
  }
}
